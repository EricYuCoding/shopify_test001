<header class="header2-nav">
  <div>
    <a href="/">
      <img
        width="100"
        height="100"
        loading="lazy"
        src="{{ 'white_background.png' | asset_url}}"
      >
    </a>
  </div>

  <div>
    <ul class="middle-nav-bar">
      {% for link in linklists['main-menu'].links %}
        <li>{{ link.title | link_to: link.url }}</li>
      {% endfor %}
    </ul>
  </div>

  <div class="right-nav-items">
    <form action="/search">
      <input class="search-nav" placeholder="search" type="search">
    </form>
    <a href="/">
      <img
        width="40"
        height="40"
        loading="lazy"
        src="{{ 'white_background.png' | asset_url}}"
      >
    </a>
  </div>
</header>
